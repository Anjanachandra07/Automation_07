package day1;

public class Solution {
	int beats;
	public int getBeats() {
		return beats;
		
	}
	
	public void setBeats(int beets)
	{
		beats=beets;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Solution ob = new Solution();
		ob.setBeats(21);
		System.out.println(ob.getBeats());

	}

}
